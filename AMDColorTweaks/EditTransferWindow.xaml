    <Grid Height="320" Width="300" Margin="8">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <TabControl Grid.Row="1" Margin="0,0,0,8" SelectedIndex="{Binding TransferType, Converter={StaticResource EnumConverter}}">
            <TabItem Header="Driver Predefined" Visibility="Collapsed">
            <!--
                <StackPanel>
                    <StackPanel.Resources>
                        <Style TargetType="RadioButton">
                            <Setter Property="Margin" Value="4,4,4,0"/>
                        </Style>
                    </StackPanel.Resources>
                    <ItemsControl ItemsSource="{Binding AvailiablePredefined}">
                        <ItemsControl.ItemTemplate>
                            <DataTemplate>
                                <RadioButton Content="{Binding}" IsChecked="{Binding IsChecked}"/>
                            </DataTemplate>
                        </ItemsControl.ItemTemplate>
                    </ItemsControl>
                </StackPanel>
            -->
            </TabItem>
            <TabItem Header="Parametric OETF">
                <DockPanel>
                    <Image DockPanel.Dock="Top" Margin="4" Source="{StaticResource Eqn}" HorizontalAlignment="Left" VerticalAlignment="Center" />
                    <TextBlock DockPanel.Dock="Top" Margin="4"><Run FontSize="16">𝐿:</Run> Linear light</TextBlock>
                    <TextBlock DockPanel.Dock="Top" Margin="4,0,4,4" ><Run FontSize="16">𝐸:</Run> Electrical signal</TextBlock>
                    <CheckBox DockPanel.Dock="Top" Margin="4" IsChecked="{Binding UseUniformParametricCurve}">Use same curve for all channels</CheckBox>
                    <DataGrid CanUserDeleteRows="False" CanUserAddRows="False" CanUserResizeRows="False" CanUserSortColumns="False" CanUserReorderColumns="False" ItemsSource="{Binding ParametricCurves}" AutoGenerateColumns="False">
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="Channel" Binding="{Binding Channel}" IsReadOnly="True"/>
                            <DataGridTextColumn Width="64" Binding="{Binding A0}">
                                <DataGridTextColumn.Header>
                                    <TextBlock FontFamily="Times New Roman">𝐴<Run FontSize="8" BaselineAlignment="Subscript">0</Run></TextBlock>
                                </DataGridTextColumn.Header>
                            </DataGridTextColumn>
                            <DataGridTextColumn Width="40" Binding="{Binding A1}">
                                <DataGridTextColumn.Header>
                                    <TextBlock FontFamily="Times New Roman">𝐴<Run FontSize="8" BaselineAlignment="Subscript">1</Run></TextBlock>
                                </DataGridTextColumn.Header>
                            </DataGridTextColumn>
                            <DataGridTextColumn Width="40" Binding="{Binding A2}">
                                <DataGridTextColumn.Header>
                                    <TextBlock FontFamily="Times New Roman">𝐴<Run FontSize="8" BaselineAlignment="Subscript">2</Run></TextBlock>
                                </DataGridTextColumn.Header>
                            </DataGridTextColumn>
                            <DataGridTextColumn Width="40" Binding="{Binding A3}">
                                <DataGridTextColumn.Header>
                                    <TextBlock FontFamily="Times New Roman">𝐴<Run FontSize="8" BaselineAlignment="Subscript">3</Run></TextBlock>
                                </DataGridTextColumn.Header>
                            </DataGridTextColumn>
                            <DataGridTextColumn Width="40" Binding="{Binding Gamma}">
                                <DataGridTextColumn.Header>
                                    <TextBlock FontFamily="Times New Roman">𝑔</TextBlock>
                                </DataGridTextColumn.Header>
                            </DataGridTextColumn>
                        </DataGrid.Columns>
                    </DataGrid>
                </DockPanel>
            </TabItem>
            <TabItem Header="Channel LUT">
                <Grid Margin="4">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <StackPanel Orientation="Horizontal">
                        <Button HorizontalAlignment="Left" Padding="3,2" Click="ReadCsvButton_Click">Load from CSV...</Button>
                        <Button HorizontalAlignment="Left" Padding="3,2" Margin="4,0,0,0" Click="SaveCsvButton_Click">Save to CSV...</Button>
                        <Button HorizontalAlignment="Left" Padding="3,2" Margin="4,0,0,0" Click="SetLinearLUTButton_Click">Set linear</Button>
                    </StackPanel>
                    <CheckBox Grid.Row="1" Margin="0,4,0,0" IsChecked="{Binding UseDegamma}">Apply degamma instead</CheckBox>
                    <Grid  Grid.Row="2">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="20"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="18"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Border Grid.Row="0" Grid.Column="1" BorderBrush="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}" BorderThickness="1" Margin="0,4,0,0">
                            <Canvas x:Name="CurveCanvas" Background="White">
                                <Canvas.Resources>
                                    <TransformGroup x:Key="CurveTransform">
                                        <ScaleTransform CenterY="1" ScaleX="{Binding ActualWidth, ElementName=CurveCanvas}" ScaleY="{Binding ActualHeight, ElementName=CurveCanvas, Converter={StaticResource NegativeNumericConverter}}"/>
                                        <TranslateTransform Y="-1"/>
                                    </TransformGroup>
                                </Canvas.Resources>
                                <Path Stroke="#80ff2020">
                                    <Path.Data>
                                        <PathGeometry Figures="{Binding RedLUT, Converter={StaticResource Lut256ToPathFigureConverter}}" Transform="{StaticResource CurveTransform}" />
                                    </Path.Data>
                                </Path>
                                <Path Stroke="#8020ff20">
                                    <Path.Data>
                                        <PathGeometry Figures="{Binding GreenLUT, Converter={StaticResource Lut256ToPathFigureConverter}}" Transform="{StaticResource CurveTransform}" />
                                    </Path.Data>
                                </Path>
                                <Path Stroke="#802020ff">
                                    <Path.Data>
                                        <PathGeometry Figures="{Binding BlueLUT, Converter={StaticResource Lut256ToPathFigureConverter}}" Transform="{StaticResource CurveTransform}" />
                                    </Path.Data>
                                </Path>
                            </Canvas>
                        </Border>
                        <TextBlock VerticalAlignment="Center" Text="Output">
                            <TextBlock.LayoutTransform>
                                <TransformGroup>
                                    <ScaleTransform/>
                                    <SkewTransform/>
                                    <RotateTransform Angle="-90"/>
                                    <TranslateTransform/>
                                </TransformGroup>
                            </TextBlock.LayoutTransform>
                        </TextBlock>
                        <TextBlock Grid.Row="1" Grid.Column="1" HorizontalAlignment="Center" Text="Input"/>
                    </Grid>
                </Grid>
            </TabItem>
        </TabControl>
        <DockPanel Grid.Row="2" LastChildFill="False">
            <Button DockPanel.Dock="Right" Height="24" Width="64" Margin="4,0,0,0" IsDefault="True" Click="CancelButton_Click">Save</Button>
            <Button DockPanel.Dock="Right" Height="24" Width="64" IsCancel="True">Cancel</Button>
        </DockPanel>
    </Grid>
</Window>
